{
    "name": "datahouse/mon-common",
    "version": "1.0.0",
    "repositories": [
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "https://repo.datalan.ch/composer/"
        }
    ],
    "require": {
        "datahouse/libraries": "7.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "pdepend/pdepend": "2.*",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "sebastian/phpcpd": "2.*",
        "theseer/phpdox": "0.*",
        "phploc/phploc": "2.*",
        "datahouse/standards": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Datahouse\\MON\\Common\\": "src/",
            "Datahouse\\MON\\Common\\Tests\\": "tests/"
        }
    }
}
